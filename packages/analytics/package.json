{
  "name": "@startupkit/analytics",
  "version": "0.1.2",
  "author": "Ian Hunter <ian@01.studio>",
  "license": "ISC",
  "description": "Analytics package for StartupKit",
  "homepage": "https://startupkit.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/01-studio/startupkit"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "source": "./src/index.ts"
    },
    "./ga": {
      "types": "./dist/ext/ga.d.ts",
      "import": "./dist/ext/ga.mjs",
      "require": "./dist/ext/ga.js",
      "source": "./src/ext/ga.ts"
    },
    "./plausible": {
      "types": "./dist/ext/plausible.d.ts",
      "import": "./dist/ext/plausible.mjs",
      "require": "./dist/ext/plausible.js",
      "source": "./src/ext/plausible.ts"
    },
    "./posthog": {
      "types": "./dist/ext/posthog.d.ts",
      "import": "./dist/ext/posthog.mjs",
      "require": "./dist/ext/posthog.js",
      "source": "./src/ext/posthog.ts"
    }
  },
  "scripts": {
    "clean": "rm -rf ./dist ./.turbo",
    "build": "tsup",
    "postbuild": "validate-package-exports --check --verify",
    "build:watch": "npx chokidar-cli \"src/**/*\" -c \"pnpm run build\" --initial",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@analytics/google-analytics": "^1.0.7",
    "@metro-fs/analytics-plugin-posthog": "^1.14.0",
    "analytics": "^0.8.13",
    "analytics-plugin-plausible": "^0.0.6",
    "posthog-js": "1.154.2",
    "react-ga": "^3.3.1"
  },
  "peerDependencies": {
    "next": "^14",
    "react": "^18.2.0"
  },
  "devDependencies": {
    "@types/mdast": "^4.0.4",
    "@types/mdx": "^2.0.13",
    "@types/next": "9",
    "@types/react": "^18.2.0",
    "@types/vfile": "^4.0.0",
    "next": "^14",
    "react": "^18.2.0",
    "validate-package-exports": "^0.6.0"
  }
}
