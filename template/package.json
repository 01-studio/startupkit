{
	"name": "",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"build": "next build",
		"db:migrate": "dotenv -e .env.local -- prisma migrate dev",
		"db:push": "dotenv -e .env.local -- prisma db push",
		"db:reset": "dotenv -e .env.local -- prisma migrate reset",
		"db:studio": "dotenv -e .env.local -- prisma studio",
		"dev": "next dev",
		"postinstall": "prisma generate",
		"lint": "npx @biomejs/biome lint ./src",
		"format": "npx @biomejs/biome check --write ./src",
		"start": "next start",
		"stripe:fixtures": "stripe fixtures src/stripe.json",
		"stripe:listen": "stripe listen --forward-to=localhost:3000/api/payments/webhook",
		"stripe:login": "stripe login",
		"typecheck": "tsc"
	},
	"dependencies": {
		"@prisma/client": "5.10.2",
		"@radix-ui/react-avatar": "^1.1.0",
		"@radix-ui/react-dialog": "^1.1.1",
		"@radix-ui/react-dropdown-menu": "^2.1.1",
		"@radix-ui/react-slot": "^1.1.0",
		"@startupkit/analytics": "workspace:*",
		"@startupkit/auth": "workspace:*",
		"@startupkit/cms": "workspace:*",
		"@startupkit/payments": "workspace:*",
		"class-variance-authority": "^0.7.0",
		"clsx": "^2.1.1",
		"framer-motion": "^11.3.24",
		"lucide-react": "^0.427.0",
		"next": "14.0.1",
		"react": "^18",
		"react-dom": "^18",
		"react-hook-form": "^7.52.2",
		"react-hot-toast": "^2.4.1",
		"tailwind-merge": "^2.4.0",
		"tailwindcss-animate": "^1.0.7"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.8.3",
		"@types/node": "^20.14.15",
		"@types/react": "^18",
		"@types/react-dom": "^18",
		"autoprefixer": "^10.4.18",
		"dotenv-cli": "^7.4.2",
		"postcss": "^8.4.35",
		"prisma": "^5.10.2",
		"tailwindcss": "^3.4.1",
		"typescript": "^5"
	}
}
